package com.scb.edmi.support;

import java.util.ArrayList;
import java.util.Map;
import java.util.Properties;

public abstract class SOAPServices {
	
	public abstract String  postSoapRequest(Properties property,String testID);
	public abstract Map<String, String> xmlRead(String pat,ArrayList<String> tagInfo);
	public abstract String getXMlValueNew(String xmlLocation,String ParentNode,String childNode,int indexOfChildNode);

}
